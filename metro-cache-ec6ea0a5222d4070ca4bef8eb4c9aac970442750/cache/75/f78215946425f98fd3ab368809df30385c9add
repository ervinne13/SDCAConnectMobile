__d(function (global, require, module, exports, _dependencyMap) {
  var baseEach = require(_dependencyMap[0], './_baseEach'),
      baseFind = require(_dependencyMap[1], './_baseFind'),
      baseFindIndex = require(_dependencyMap[2], './_baseFindIndex'),
      baseIteratee = require(_dependencyMap[3], './_baseIteratee'),
      isArray = require(_dependencyMap[4], './isArray');

  function find(collection, predicate) {
    predicate = baseIteratee(predicate, 3);

    if (isArray(collection)) {
      var index = baseFindIndex(collection, predicate);
      return index > -1 ? collection[index] : undefined;
    }

    return baseFind(collection, predicate, baseEach);
  }

  module.exports = find;
});